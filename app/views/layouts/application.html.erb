<!DOCTYPE html>
<html>
  <head>
    <title>Eventbook</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <script type="text/javascript">
       window.images = {
         profile_icon: "<%= asset_path('profile_icon.png')%>",
         party_pic: "<%= asset_path('party_pic.jpg')%>",
         search_icon: "<%= asset_path('search_icon.png')%>",
         unliked: "<%= asset_path('unliked.png')%>",
         liked: "<%= asset_path('liked.png')%>",
         basic_details_icon: "<%= asset_path('basic_details_icon.png')%>",
         location_icon: "<%= asset_path('location_icon.png')%>",
         date_time_icon: "<%= asset_path('date_time_icon.png')%>",
         picture_icon: "<%= asset_path('picture_icon.png')%>"

       }
     </script>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag asset_path('favicon.png'), :rel => 'icon', :type => 'image/png' %>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
